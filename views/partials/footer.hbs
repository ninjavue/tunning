<script>
    let searchList =  document.querySelector('.search__list')

    fetch('/profile/msg/notif')
    .then(response => response.json())
    .then(notif => {
        if (notif !==0) {
            document.querySelector('.msgs_count').style.display = 'flex'
            document.querySelector('.msgs_count').textContent = notif
        } else {
            document.querySelector('.msgs_count').style.display = 'none'
        }
    })

    function searchApi(e){
        if (e.target.value.length>0) {
            fetch('/ads/search/' + e.target.value)
                    .then(response => response.json())
                    .then(data => {
                        searchList.innerHTML = ''
                        data.forEach(ads => {
                            searchList.innerHTML += `
                            <a href='/ads/${ads._id}'>
                                <span class='img' style="background-image: url('/${ads.photo}')"></span>
                                   ${ads.title}
                            </a>
                        `
                        })
                    }).catch((e)=>{
                        searchList.innerHTML=''
                        console.log(e)
                    })
        } else {
            searchList.innerHTML = ''
        }
    }

    fetch('/category/api/all')
            .then(res=>res.json())
            .then(category => {
                document.querySelector('.cats__list').innerHTML = ''
                category.forEach(cat => {
                    document.querySelector('.cats__list').innerHTML+= `
                        <div class="item">
                    <a href="/category/${cat._id}" class="title_item">
                        <span class="bc_item" style="background-image: url(/${cat.img})">

                    </span>
                    <div class="bc_text">${cat.name}</div>
                    </a>
                    </div>
                    `
                })
            }).catch((e)=>{ console.log(e) })
</script>